    <!-- common : 通用 -->
    <item name="main_anim_duration" format="integer" type="dimen">250</item>  
    
    <!-- APP : Reader -->
    <dimen name="reader_bootom_button_unit_textsize">14sp</dimen>
    <dimen name="reader_bootom_button_menu_textsize">12sp</dimen>
    
</resources>
